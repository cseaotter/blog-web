etSummary(postInfo.summary);\n      })\n    );\n  }, []);\n\n  async function updatePost(ev) {\n    ev.preventDefault();\n    const data = new FormData();\n    data.append(\"title\", title);\n    data.append(\"summary\", summary);\n    data.append(\"content\", content);\n    data.append('id', id);\n\n    if (files?.[0]) {\n      data.append(\"file\", files?.[0]);\n    }\n\n    const response = await fetch(\"http://localhost:4000/post\", {\n      method: \"PUT\",\n      body: data,\n      credentials:\"include\",\n    });\n\n    if (response.ok) {\n        setRedirect(true);\n    }\n  }\n\n  if (redirect) {\n    if (id != null) {\n        return <Navigate to={\"/post/\" + id} />;\n    } else {\n        return <Navigate to={\"/\"} />;\n    }\n  }\n\n  return (\n    <form onSubmit={updatePost}>\n      <input\n        type=\"title\"\n        placeholder={\"Title\"}\n        value={title}\n        onChange={(ev) => setTitle(ev.target.value)}\n      />\n      <input\n        type=\"summary\"\n        placeholder={\"Summary\"}\n        value={summary}\n        onChange={(ev) => setSummary(ev.target.value)}\n      />\n      <input type=\"file\" onChange={(ev) => setFiles(ev.target.files)} />\n      <Editor value={content} onChange={setContent} />\n      <button style={ { marginTop: \"5px\" }}>Update post</button>\n    </form>\n  );\n}\n",[],"/Users/carolgao/Code/blog-website/client/src/Editor.js",[],[],{"ruleId":"82","severity":1,"message":"83","line":26,"column":11,"nodeType":"84","endLine":26,"endColumn":33},{"ruleId":"85","severity":1,"message":"86","line":15,"column":6,"nodeType":"87","endLine":15,"endColumn":8,"suggestions":"88"},{"ruleId":"82","severity":1,"message":"83","line":36,"column":13,"nodeType":"84","endLine":36,"endColumn":33},{"ruleId":"89","severity":1,"message":"90","line":27,"column":21,"nodeType":"91","messageId":"92","endLine":27,"endColumn":23},{"ruleId":"85","severity":1,"message":"93","line":22,"column":6,"nodeType":"87","endLine":22,"endColumn":8,"